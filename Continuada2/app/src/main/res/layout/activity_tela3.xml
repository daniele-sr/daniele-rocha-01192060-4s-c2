<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tela3">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cachorro1"
        android:textSize="30dp"
        android:id="@+id/et_um"
        />

    <TextView
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:id="@+id/tv_resultado"
        app:layout_constraintLeft_toRightOf="@+id/et_um"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cachorro2"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/et_um"
        android:id="@+id/tv_dois"
        />

    <TextView
        android:layout_width="50dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:id="@+id/tv_resultado2"
        app:layout_constraintLeft_toRightOf="@+id/tv_dois"
        app:layout_constraintTop_toBottomOf="@+id/tv_resultado"
        android:layout_marginTop="20dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/preco"
        android:textSize="30dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_resultado2"
        android:id="@+id/tv_preco"
        />

    <TextView
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:id="@+id/tv_resultado3"
        app:layout_constraintLeft_toRightOf="@+id/tv_preco"
        app:layout_constraintTop_toBottomOf="@+id/tv_resultado2"
        android:layout_marginTop="10dp"
        />


</androidx.constraintlayout.widget.ConstraintLayout>